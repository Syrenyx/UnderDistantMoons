{
  "multipart": [
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/center_caps"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/center_post"
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/cap/default"
      },
      "when": {
        "east": "false",
        "north": "true",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/cap/default",
        "y": 90
      },
      "when": {
        "east": "true",
        "north": "false",
        "south": "false",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/cap/mirrored",
        "y": 180
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "true",
        "west": "false"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/cap/mirrored",
        "y": 270
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/side/default"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/side/default",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/side/mirrored",
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/refined_deep_iron_bars/side/mirrored",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    }
  ]
}