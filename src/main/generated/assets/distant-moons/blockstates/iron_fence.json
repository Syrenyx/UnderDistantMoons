{
  "multipart": [
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/pole"
      },
      "when": {
        "top": "false"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/top_pole"
      },
      "when": {
        "top": "true"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/side"
      },
      "when": {
        "north": "side"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/side",
        "y": 90
      },
      "when": {
        "east": "side"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/side",
        "y": 180
      },
      "when": {
        "south": "side"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/side",
        "y": 270
      },
      "when": {
        "west": "side"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/top_side"
      },
      "when": {
        "north": "top"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/top_side",
        "y": 90
      },
      "when": {
        "east": "top"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/top_side",
        "y": 180
      },
      "when": {
        "south": "top"
      }
    },
    {
      "apply": {
        "model": "distant-moons:object/iron_fence/top_side",
        "y": 270
      },
      "when": {
        "west": "top"
      }
    }
  ]
}